package test_p15;

public class Sample3_7 {
	public static void main(String[] args)
	{
		Car car1;
		car1 = new Car();
		
		car1.setNum(1234);
		car1.setGas(20.5);
		
		
	}
}
class Car
{
	int num;
	double gas;
	void setNum(int n,double g)
	{
		num = n;
		gas = g;
		System.out.println("將車號設為"+num+",將汽油量設為"+gas);
	}
	void show()
	{
		System.out.println("車號是"+num+"\n汽油量是"+gas);
	}
}
